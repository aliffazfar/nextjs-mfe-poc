{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "pnpm run dev:host & pnpm run dev:login & pnpm run dev:profile",
    "dev:host": "pnpm --filter host dev",
    "dev:login": "pnpm --filter login dev",
    "dev:profile": "pnpm --filter profile dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2",
    "webpack": "^5.102.1"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
